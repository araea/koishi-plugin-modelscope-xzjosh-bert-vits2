{
  "name": "koishi-plugin-modelscope-xzjosh-bert-vits2",
  "description": "提供基于 ModelScope 平台 [xzjosh bert-vits2](https://www.modelscope.cn/studios/xzjosh/Bert-VITS2) 模型的 AI 语音合成功能，支持 vits 服务和多种角色声音，例如东雪莲、丁真。",
  "version": "0.0.6",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "dist"
  ],
  "koishi": {
    "service": {
      "required": [
        "database"
      ],
      "optional": [
        "monetary"
      ]
    },
    "implements": [
      "vits"
    ]
  },
  "license": "MIT",
  "author": "Nawyjx <3373167460@qq.com>",
  "homepage": "https://www.npmjs.com/package/koishi-plugin-modelscope-xzjosh-bert-vits2?activeTab=readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/araea/koishi-plugin-modelscope-xzjosh-bert-vits2.git"
  },
  "keywords": [
    "chatbot",
    "koishi",
    "plugin",
    "modelscope",
    "xzjosh",
    "bert",
    "vits2",
    "ai",
    "voice",
    "tts",
    "text-to-speech",
    "语音合成",
    "文本转语音"
  ],
  "peerDependencies": {
    "koishi": "^4.17.9"
  },
  "devDependencies": {
    "koishi-plugin-monetary": "^0.1.3"
  }
}
